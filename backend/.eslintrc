{
  "extends": "airbnb-base",
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-underscore-dangle": "off",
    "prefer-promise-reject-errors": "off",
    "prefer-destructuring": [
      "error",
      {
        "VariableDeclarator": {
          "array": false,
          "object": false
        }
      }
    ],
  }
}